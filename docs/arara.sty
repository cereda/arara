\newtcbox{\abox}[1][teal]{
  on line,
  arc=3pt,
  colback=#1!10,
  tcbox raise=-1.1mm,
  colframe=#1,
  before upper={
    \vphantom{dp}
  },
  boxrule=1pt,
  fontupper=\footnotesize\normalfont\ttfamily\color{#1},
  boxsep=0pt,
  left=2pt,
  right=2pt,
  top=1.6pt,
  bottom=0.8pt
}

